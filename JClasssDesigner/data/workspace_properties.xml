<?xml version="1.0" encoding="UTF-8"?>
<!--
    This XML file allows us to easily customize the look and language used
    in our program's user interface. Note that this properties file is only
    for the workspace controls, which are custom for each application
-->
<properties>
    <property_list>
        <!-- PROGRAM ICON FILES -->
	<property name="SELECT_TOOL_ICON"	value="Select.png"></property>
        <property name="RESIZE_ICON"		value="Resize.png"></property>
	<property name="REMOVE_ICON"		value="Remove.png"></property>
        <property name="ADD_CLASS_ICON"		value="AddClass.png"></property>
	<property name="ADD_INTERFACE_ICON"	value="AddInterface.png"></property>
	<property name="ADD_VARIABLE_ICON"	value="AddVariableMethod.png"></property>
	<property name="ADD_METHOD_ICON"	value="AddVariableMethod.png"></property>
	<property name="REMOVE_VARIABLE_ICON"        value="RemoveVariableMethod.png"></property>
	<property name="REMOVE_METHOD_ICON"		value="RemoveVariableMethod.png"></property>
	<property name="ZOOMIN_ICON"		value="ZoomOut.png"></property>
	<property name="ZOOMOUT_ICON"		value="ZoomIn.png"></property>
	<property name="REDO_ICON"		value="Redo.png"></property>
	<property name="UNDO_ICON"		value="Undo.png"></property>

        <!-- TOOLTIPS FOR BUTTONS -->
	<property name="SELECT_TOOL_TOOLTIP"	    value="Use Class Selection Tool"></property>
        <property name="RESIZE_TOOLTIP"		value="Resize"></property>
        <property name="REMOVE_TOOLTIP"             value="Remove Class"></property>
	<property name="ADD_CLASS_TOOLTIP"	    value="Add New Class"></property>
	<property name="ADD_INTERFACE_TOOLTIP"	    value="Add New Interface"></property>
	<property name="ADD_VARIABLE_TOOLTIP"	    value="Add Variable"></property>
	<property name="REMOVE_VARIABLE_TOOLTIP"    value="Remove Variable"></property>
	<property name="ADD_METHOD_TOOLTIP"	    value="Add Method"></property>
	<property name="REMOVE_METHOD_TOOLTIP"      value="Remove Method"></property>
	<property name="ZOOMIN_TOOLTIP"             value="Zoom In"></property>
	<property name="ZOOMOUT_TOOLTIP"	    value="Zoom Out"></property>
	<property name="REDO_TOOLTIP"	    value="Redo"></property>
	<property name="UNDO_TOOLTIP"	    value="Undo"></property>
	
	<!-- MESSAGES -->
	<property name="REMOVE_ELEMENT_MESSAGE"	    value="Are you sure you wish to remove that element?"></property>
		
	<!-- ERROR MESSAGES AND TITLES FOR THEIR DIALOGS -->
	<property name="ATTRIBUTE_UPDATE_ERROR_MESSAGE"	    value="WARNING: An error occured updating the element's attribute."></property>
	<property name="ATTRIBUTE_UPDATE_ERROR_TITLE"	    value="Attribute Update Error"></property>
	<property name="UPDATE_ERROR_MESSAGE"		    value="An error occured updating the UI"></property>
	<property name="UPDATE_ERROR_TITLE"		    value="UI Update Error"></property>
    </property_list>
    <property_options_list>
    </property_options_list>
</properties>